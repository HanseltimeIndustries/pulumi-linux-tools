{
	"name": "@hanseltime/pulumi-linux-docker",
	"version": "1.2.0",
	"description": "pulumi TS component resources for docker deployments on a linux machine with SSH",
	"license": "MIT",
	"types": "./dist/types/index.d.ts",
	"main": "./dist/cjs/index.js",
	"exports": {
		"types": "./dist/types/index.d.ts",
		"require": "./dist/cjs/index.js",
		"import": "./dist/esm/index.mjs",
		"default": "./dist/esm/index.mjs"
	},
	"files": [
		"dist",
		"assets"
	],
	"devDependencies": {
		"@biomejs/biome": "^2.0.4",
		"@commitlint/config-angular": "^18.4.3",
		"@hanseltime/esm-interop-tools": "^1.0.3",
		"@hanseltime/pkgtest": "^1.3.0",
		"@hanseltime/swc-plugin-node-globals-inject": "^1.0.0",
		"@pulumi/command": "^1.0.3",
		"@pulumi/pulumi": "^3.158.0",
		"@pulumiverse/grafana": "^0.16.3",
		"@swc/cli": "^0.5.2",
		"@swc/core": "^1.10.1",
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.4",
		"commitlint": "^18.4.3",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-chain-transform": "^0.0.8",
		"ts-jest": "^29.1.1",
		"tswc": "^1.4.0",
		"tsx": "^4.19.2",
		"typedoc": "^0.28.1",
		"typedoc-plugin-markdown": "^4.6.0",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"@pulumi/command": "^1.0.3",
		"@pulumi/pulumi": "^3.158.0"
	},
	"optionalDependencies": {
		"@pulumiverse/grafana": "^0.16.3"
	},
	"scripts": {
		"test": "jest",
		"lint": "biome lint",
		"format": "biome format",
		"build:esm": "tswc -- src -d dist/esm --config-file .esm.swcrc --strip-leading-paths --out-file-extension mjs",
		"build:cjs": "tswc -- src -d dist/cjs --config-file .cjs.swcrc --strip-leading-paths",
		"build:bin": "node ./bin-build.js",
		"build:types": "tsc",
		"build": "yarn build:types && yarn build:esm && yarn build:cjs && yarn build:bin",
		"typedoc": "typedoc"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "yarn@4.7.0",
	"dependencies": {
		"@hanseltime/pulumi-file-utils": "workspace:^",
		"@hanseltime/pulumi-linux": "workspace:^",
		"@hanseltime/pulumi-linux-base": "workspace:^",
		"@hanseltime/pulumi-linux-iptables": "workspace:^",
		"@hanseltime/traefik": "workspace:^",
		"node-fetch": "^3.3.2"
	}
}
